@model ClientService.Models.PeopleModelList

@{
    ViewBag.Title = "Employee";
}

<h2>Employee List</h2>
    <table>
      <tr>
        <th>ID </th>
        <th>Name</th>
        <th>Country</th>
        <th>Contact</th>
        <th>Email</th>
      </tr>
   @foreach (var item in Model) {
       <tr>
            <td>
               @Html.DisplayFor(model => item.ID) 
           </td>
           <td>
               @Html.DisplayFor(model => item.Name) 
           </td>
           <td>
               @Html.DisplayFor(model => item.Country)
           </td>
           <td>
               @Html.DisplayFor(model => item.Contact) 
           </td>
           <td>
               @Html.DisplayFor(model => item.Email)
           </td>
       </tr>
         }
    </table>  